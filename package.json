{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json && node build/server.js",
    "start": "nodemon --legacy-watch src/server.ts --exitcrash",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm-m": "ts-node ./node_modules/typeorm/cli migration:generate -n test "
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "-": "^0.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^16.11.13",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "fastify": "^3.24.1",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "2.0.15",
    "npm": "^8.3.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.41",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "chai": "4.3.4",
    "debug": "4.3.2",
    "dirty-chai": "2.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.0.0",
    "eslint-config-airbnb-typescript-base": "^2.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^4.3.8",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "prettier": "2.4.1",
    "supertest": "6.1.6"
  }
}
